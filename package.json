{
  "name": "fivem-devbridge",
  "displayName": "FiveM DevBridge",
  "description": "ensure resource when you save any change",
  "publisher": "DuckWichTrust",
  "version": "0.0.4",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:easy-fivem.connect",
    "onCommand:easy-fivem.disconnect",
    "onCommand:easy-fivem.customConnect",
    "onCommand:easy-fivem.safeConnect",
    "onCommand:easy-fivem.connectSaved"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "easy-fivem.connect",
        "title": "fivem connect"
      },
      {
        "command": "easy-fivem.disconnect",
        "title": "fivem disconnect"
      },
      {
        "command": "easy-fivem.customConnect",
        "title": "fivem custom-connect"
      },
      {
        "command": "easy-fivem.safeConnect",
        "title": "Safe Connect to FiveM"
      },
      {
        "command": "easy-fivem.connectSaved",
        "title": "Connect to Saved Connection"
      },
      {
        "command": "easy-fivem.setCurrentResource",
        "title": "Set as Current Resource"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "easy-fivem.setCurrentResource",
          "when": "resourceIsFolder"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.71.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "rcon": "^1.1.0"
  }
}